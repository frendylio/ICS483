% allows for Example 4(c) 
% \begin{subexa}{ame_3}
% \label{second_part}
% \end{subexa}
% Example~\ref{ame_3}\ref{second_part} % yields: Example 3(b)

\def\subexa#1%
{
  \@ifundefined{c@#1}       % c@: allwas pre-pended by definecounter
  {     
    \@definecounter{#1}     % \definecounter: defines and sets to 0
    \expandafter\gdef\csname the#1\endcsname{\alph{#1}} 
                % \expandafter: 
                % \gdef: global - valid outside {}
                % \csname xxx \endscname: makes one word
                % \alph: alphabetic representation
    \@definecounter{main@#1}
    \refstepcounter{Example}    % \ref..: make it the counter used by \label
    \label{#1}
    \setcounter{main@#1}{\value{Example}}
  }
  \relax            % relax: 2nd argument (to ifundefined) empty
  \refstepcounter{#1}
  \trivlist
  \item[\hskip\labelsep{\bf Example~\arabic{main@#1}(\csname the#1\endcsname)}]
}
\endtrivlist

