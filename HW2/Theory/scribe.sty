\newdimen\mittopsepiv

\mittopsepi   \topsep
\mittopsepii  \topsep
\mittopsepiii \topsep
\mittopsepiv  \topsep

\def\mit{\ifnum \@mitdepth >3 \@toodeep\else \advance\@mitdepth\@ne
         \edef\@mitlab{mitlabel\romannumeral\the\@mitdepth}
         \edef\@mitmar{mitleftmargin\romannumeral\the\@mitdepth}
         \edef\@mitite{mititemsep\romannumeral\the\@mitdepth}
         \edef\@mittop{mittopsep\romannumeral\the\@mitdepth}
         \list{\csname\@mitlab\endcsname}
              {\leftmargin\csname\@mitmar\endcsname
               \itemsep\csname\@mitite\endcsname
               \topsep\csname\@mittop\endcsname
               \def\makelabel##1{\hss\llap{##1}}}\fi
        }
\let\endmit =\endlist

